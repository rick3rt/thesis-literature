  keywords = {Algorithms,biomedical ultrasonics,Elasticity Imaging Techniques,Humans,Image resolution,Ultrasonic imaging,Array signal processing,Elasticity,Image Enhancement,Models; Statistical,transient elastography,Ultrasonography,Biological tissues,Image quality,Data acquisition,Signal Processing; Computer-Assisted,beamforming process,biological tissues,coherent plane-wave compounding,coherent recombination,compounded plane-wave transmissions,conventional B-mode imaging,Data visualization,echographic images,Female,insonifications,Models; Theoretical,phantoms,tissue-mimicking phantoms,ultrafast frame,ultrasonography,Ultrasonography; Mammary,Viscosity},
  pages = {489-506},
  file = {C:\\Users\\rickw\\OneDrive\\Studie\\BMD_Master\\Thesis\\Zotero\\storage\\HLSETZIK\\Montaldo et al_2009_Coherent plane-wave compounding for very high frame rate ultrasonography and.pdf},
