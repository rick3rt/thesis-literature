  file = {C:\\Users\\rickw\\OneDrive\\Studie\\BMD_Master\\Thesis\\Zotero\\storage\\Z72JINMB\\Markowitz et al_2011_Speed adaptation in a powered transtibial prosthesis controlled with a.pdf},
  pmid = {21502131},
  note = {00075}
}

@article{madeleine_mechanomyography_2001,
  title = {Mechanomyography and Electromyography Force Relationships during Concentric, Isometric and Eccentric Contractions},
  volume = {11},
  issn = {1050-6411},
  doi = {10.1016/S1050-6411(00)00044-4},
  abstract = {The purpose of this study was to investigate systematically if complementary knowledge could be obtained from the recordings of electromyography (EMG) and mechanomyography (MMG) signals. EMG and MMG activities were recorded from the first dorsal interosseous muscle during slow concentric, isometric, and eccentric contraction at 0, 25, 50, 75 and 100\% of the maximal voluntary contraction (MVC). The combination of the EMG and MMG recordings during voluntary concentric\textendash{}isometric\textendash{}eccentric contraction showed significant different non-linear EMG/force and MMG/force relationships (P$<$0.001). The EMG root mean square (rms) values increased significantly from 0 to 50\% MVC during concentric and isometric contraction and up to 75\% MVC during eccentric contraction (P$<$0.05). The MMG rms values increased significantly from 0 to 50\% MVC during concentric contraction (P$<$0.05). The non-linear relationships depended mainly on the type and the level of contraction together with the angular velocity. Furthermore, the type of contraction, the contraction level, and the angular velocity influenced the electromechanical efficiency evaluated as the MMG to EMG ratio (P$<$0.05). These results highlight that EMG and MMG provide complementary information about the electrical and mechanical activity of the muscle. Different activation strategies seem to be used during graded isometric and anisometric contraction.},
  number = {2},
  journal = {Journal of Electromyography and Kinesiology},
  author = {Madeleine, P and Bajaj, P and S\o{}gaard, K and {Arendt-Nielsen}, L},
  month = apr,
  year = {2001},
  keywords = {Electromyography,Isometric and anisometric contraction,Mechanomyography},
  pages = {113-121},
  file = {C:\\Users\\rickw\\OneDrive\\Studie\\BMD_Master\\Thesis\\Zotero\\storage\\R4LMGU52\\Madeleine et al_2001_Mechanomyography and electromyography force relationships during concentric,.pdf},
  note = {00159}
}

@article{cramer_acute_2005,
  title = {The Acute Effects of Static Stretching on Peak Torque, Mean Power Output, Electromyography, and Mechanomyography},
  volume = {93},
  issn = {1439-6327},
  doi = {10.1007/s00421-004-1199-x},
  abstract = {The purpose of this study was to examine the acute effects of static stretching on peak torque (PT), the joint angle at PT, mean power output (MP), electromyographic (EMG) amplitude, and mechanomyographic (MMG) amplitude of the vastus lateralis (VL) and rectus femoris (RF) muscles during maximal, voluntary concentric isokinetic leg extensions at 60 and 240$^\circ\cdot$s-1 of the stretched and unstretched limbs. Twenty-one volunteers [mean age (SD) 21.5 (1.3) years] performed maximal, voluntary concentric isokinetic leg extensions for the dominant and non-dominant limbs at 60 and 240$^\circ\cdot$s-1. Surface EMG ($\mu$Vrms) and MMG (mVrms) signals were recorded from the VL and RF muscles during the isokinetic tests. PT (Nm), the joint angle at PT, and MP (W) were calculated by a dynamometer. Following the initial isokinetic tests, the dominant leg extensors were stretched using four static stretching exercises. After the stretching, the isokinetic tests were repeated. PT decreased (P$\leq$0.05) from pre- to post-stretching for the stretched limb at 60 and 240$^\circ\cdot$s-1 and for the unstretched limb at 60$^\circ\cdot$s-1. EMG amplitude of the VL and RF also decreased (P$\leq$0.05) from pre- to post-stretching for the stretched and unstretched limbs. There were no stretching-induced changes (P$>$0.05) for the joint angle at PT, MP, or MMG amplitude. These findings indicated stretching-induced decreases in force production and muscle activation. The decreases in PT and EMG amplitude for the unstretched limb suggested that the stretching-induced decreases may be due to a central nervous system inhibitory mechanism.},
  language = {en},
  number = {5},
  journal = {European Journal of Applied Physiology},
  author = {Cramer, J. T. and Housh, T. J. and Weir, J. P. and Johnson, G. O. and Coburn, J. W. and Beck, T. W.},
  month = mar,
  year = {2005},
  keywords = {Electromyography,Mechanomyography,Contralateral,Quadriceps femoris,Stretch},
  pages = {530-539},
  file = {C:\\Users\\rickw\\OneDrive\\Studie\\BMD_Master\\Thesis\\Zotero\\storage\\99WD7HZN\\Cramer et al_2005_The acute effects of static stretching on peak torque, mean power output,.pdf},
