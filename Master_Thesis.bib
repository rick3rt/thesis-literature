  file = {C:\\Users\\rickw\\OneDrive\\Studie\\BMD_Master\\Thesis\\Zotero\\storage\\8FHVZZMP\\Denarie et al. - 2013 - Coherent Plane Wave Compounding for Very High Fram.pdf},
