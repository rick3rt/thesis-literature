@article{meskers_neurocontrol_2015,
  title = {{{NeuroControl}} of Movement: System Identification Approach for Clinical Benefit},
  volume = {9},
  issn = {1662-5145},
  shorttitle = {{{NeuroControl}} of Movement},
  doi = {10.3389/fnint.2015.00048},
  abstract = {Progress in diagnosis and treatment of movement disorders after neurological diseases like stroke, cerebral palsy, dystonia and at old age requires understanding of the altered capacity to adequately respond to physical obstacles in the environment. With posture and movement disorders, the control of muscles is hampered, resulting in aberrant force generation and improper impedance regulation. Understanding of this improper regulation not only requires the understanding of the role of the neural controller, but also attention for the 1) the interaction between the neural controller and the ``plant'', comprising the biomechanical properties of the skeleton including the viscoelastic properties of the contractile (muscle) and non-contractile (connective) tissues: neuromechanics and 2) the closed loop nature of neural controller and biomechanical system in which cause and effect interact and are hence difficult to separate. Properties of the neural controller and the biomechanical system need to be addressed synchronously by the combination of haptic robotics, (closed loop) system identification, and neuro-mechanical modelling. In this paper, we argue that assessment of neuromechanics in response to well defined environmental conditions and tasks may provide for key parameters to understand posture and movement disorders in neurological diseases and as biomarkers to increase accuracy of prediction models for functional outcome and effects of intervention.},
  language = {English},
  journal = {Frontiers in Integrative Neuroscience},
  author = {Meskers, Carel G. M. and {de Groot}, Jurriaan and {de Vlugt}, Erwin and Schouten, Alfred},
  year = {2015},
  keywords = {afferent feedback modulation,Ageing,Movement Disorders,neuromechanics,Stroke,system identification},
  file = {C:\\Users\\rickw\\OneDrive\\Studie\\BMD_Master\\Thesis\\Zotero\\storage\\CVNXPBAL\\Meskers et al_2015_NeuroControl of movement.pdf},
  note = {00012}
}

@article{cartwright_evidence-based_2012,
  title = {Evidence-Based Guideline: {{Neuromuscular}} Ultrasound for the Diagnosis of Carpal Tunnel Syndrome},
