  author = {{de Gooijer-van de Groep}, Karin L. and {de Vlugt}, Erwin and {van der Krogt}, Hanneke J. and Helgad\'ottir, \'Ar\'ora and Arendzen, J. Hans and Meskers, Carel G. M. and {de Groot}, Jurriaan H.},
  month = jun,
  year = {2016},
  keywords = {Biomechanics,Stiffness,Stroke,Neuromuscular modeling,Assessment,Spasticity,read1},
  pages = {93-101},
  file = {C:\\Users\\rickw\\OneDrive\\Studie\\BMD_Master\\Thesis\\Zotero\\storage\\8H2YPMY9\\de Gooijer-van de Groep et al_2016_Estimation of tissue stiffness, reflex activity, optimal muscle length and.pdf;C:\\Users\\rickw\\OneDrive\\Studie\\BMD_Master\\Thesis\\Zotero\\storage\\HBV6HT3R\\appendix_A_model_de_gooijer-van_de_groep_estimation_2016.docx;C:\\Users\\rickw\\OneDrive\\Studie\\BMD_Master\\Thesis\\Zotero\\storage\\RTXZMN45\\S0268003316300274.html},
  note = {00000}
}

