  journal = {Archives of Physical Medicine and Rehabilitation},
  author = {Selles, Ruud W. and Li, Xiaoyan and Lin, Fang and Chung, Sun G. and Roth, Elliot J. and Zhang, Li-Qun},
  month = dec,
  year = {2005},
  keywords = {Biomechanics,Cerebrovascular accident,Muscle spasticity,Physical therapy,Rehabilitation},
