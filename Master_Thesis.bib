  month = jun,
  year = {2006},
  keywords = {Reflexes,Admittance,Haptic interface,Joint dynamics,Wrist},
  pages = {134-141},
  file = {C:\\Users\\rickw\\OneDrive\\Studie\\BMD_Master\\Thesis\\Zotero\\storage\\I38IGQCD\\Schouten et al_2006_Design of a torque-controlled manipulator to analyse the admittance of the.pdf;C:\\Users\\rickw\\OneDrive\\Studie\\BMD_Master\\Thesis\\Zotero\\storage\\MEFQ9RC7\\S0165027005004498.html},
  note = {00056}
}

@article{vlugt_closed-loop_2003,
  title = {Closed-Loop Multivariable System Identification for the Characterization of the Dynamic Arm Compliance Using Continuous Force Disturbances: A Model Study},
