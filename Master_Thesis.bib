  pages = {12-20},
  file = {C:\\Users\\rickw\\OneDrive\\Studie\\BMD_Master\\Thesis\\Zotero\\storage\\QGKR8WCY\\de Mos et al. - 2007 - The incidence of complex regional pain syndrome A.pdf},
