  month = feb,
  year = {2009},
  keywords = {Transient elastography,Ultrasound,Liver fibrosis,Shear wave imaging},
  pages = {219-229},
  file = {C:\\Users\\rickw\\OneDrive\\Studie\\BMD_Master\\Thesis\\Zotero\\storage\\NQFN7HTX\\Muller et al_2009_Quantitative Viscoelasticity Mapping of Human Liver Using Supersonic Shear.pdf;C:\\Users\\rickw\\OneDrive\\Studie\\BMD_Master\\Thesis\\Zotero\\storage\\W4947XAB\\S0301562908003906.html},
