@article{grey_positive_2007,
  title = {Positive Force Feedback in Human Walking},
  volume = {581},
  issn = {1469-7793},
  doi = {10.1113/jphysiol.2007.130088},
  abstract = {The objective of this study was to determine if load receptors contribute to the afferent-mediated enhancement of ankle extensor muscle activity during the late stance phase of the step cycle. Plantar flexion perturbations were presented in late stance while able-bodied human subjects walked on a treadmill that was declined by 4\%, inclined by 4\% or held level. The plantar flexion perturbation produced a transient, but marked, presumably spinally mediated decrease in soleus EMG that varied directly with the treadmill inclination. Similarly, the magnitude of the control step soleus EMG and Achilles' tendon force also varied directly with the treadmill inclination. In contrast, the ankle angular displacement and velocity were inversely related to the treadmill inclination. These results suggest that Golgi tendon organ feedback, via the group Ib pathway, is reduced when the muscle\textendash{}tendon complex is unloaded by a rapid plantar flexion perturbation in late stance phase. The changes in the unload response with treadmill inclination suggest that the late stance phase soleus activity may be enhanced by force feedback.},
  language = {en},
  number = {1},
  journal = {The Journal of Physiology},
  author = {Grey, Michael J. and Nielsen, Jens Bo and Mazzaro, Nazarena and Sinkj\ae{}r, Thomas},
  month = may,
