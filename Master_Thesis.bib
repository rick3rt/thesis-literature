
@article{tanter_ultrafast_2014,
  title = {Ultrafast Imaging in Biomedical Ultrasound},
  volume = {61},
  issn = {0885-3010},
  doi = {10.1109/TUFFC.2014.2882},
  abstract = {Although the use of ultrasonic plane-wave transmissions rather than line-per-line focused beam transmissions has been long studied in research, clinical application of this technology was only recently made possible through developments in graphical processing unit (GPU)-based platforms. Far beyond a technological breakthrough, the use of plane or diverging wave transmissions enables attainment of ultrafast frame rates (typically faster than 1000 frames per second) over a large field of view. This concept has also inspired the emergence of completely novel imaging modes which are valuable for ultrasound-based screening, diagnosis, and therapeutic monitoring. In this review article, we present the basic principles and implementation of ultrafast imaging. In particular, present and future applications of ultrafast imaging in biomedical ultrasound are illustrated and discussed.},
  language = {en},
  number = {1},
  journal = {IEEE Transactions on Ultrasonics, Ferroelectrics, and Frequency Control},
  author = {Tanter, Mickael and Fink, Mathias},
  month = jan,
  year = {2014},
  pages = {102-119},
  file = {C:\\Users\\rickw\\OneDrive\\Studie\\BMD_Master\\Thesis\\Zotero\\storage\\E8PZJJTS\\Tanter and Fink - 2014 - Ultrafast imaging in biomedical ultrasound.pdf},
  note = {00375}
}

@article{cronin_triceps_2015,
  title = {Triceps Surae Fascicle Stretch Is Poorly Correlated with Short Latency Stretch Reflex Size},
  volume = {52},
  issn = {1097-4598},
  doi = {10.1002/mus.24538},
  abstract = {Introduction: The short latency stretch reflex (SLR) is well described, but the stimulus that evokes the SLR remains elusive. One hypothesis states that reflex size is proportional to muscle fiber stretch, so in this study we examined the relationship between these 2 parameters in human triceps surae muscles. Methods: Achilles tendon taps and dorsiflexion stretches with different amplitudes and preactivation torques were applied to 6 participants while electromyography and muscle fascicle length changes were recorded in soleus and medial gastrocnemius (MG). Results: In response to tendon taps, neither fascicle length nor velocity changes were correlated with SLR size in either muscle, but accelerometer peaks were observed immediately after hammer\textendash{}tendon contact. Similar results were obtained after dorsiflexion stretches. Conclusion: Muscle fascicle stretch is poorly correlated with SLR size, regardless of perturbation parameters. We attribute the SLR trigger to the transmission of vibration through the lower limb, rather than muscle fiber stretch. Muscle Nerve 52: 245\textendash{}251, 2015},
  language = {en},
  number = {2},
  journal = {Muscle \& Nerve},
  author = {Cronin, Neil J. and Rantalainen, Timo and Avela, Janne},
  month = aug,
  year = {2015},
  keywords = {dorsiflexion stretch,muscle fascicle,reflex EMG,tendon tap,ultrasound,read3},
  pages = {245-251},
  file = {C:\\Users\\rickw\\OneDrive\\Studie\\BMD_Master\\Thesis\\Zotero\\storage\\FH4BS32Y\\Cronin et al. - 2015 - Triceps surae fascicle stretch is poorly correlate.pdf;C:\\Users\\rickw\\OneDrive\\Studie\\BMD_Master\\Thesis\\Zotero\\storage\\TZA76N57\\mus.html},
  note = {00005}
}

@article{schuurmans_monosynaptic_2009,
  title = {The Monosynaptic {{Ia}} Afferent Pathway Can Largely Explain the Stretch Duration Effect of the Long Latency {{M2}} Response},
  volume = {193},
  issn = {1432-1106},
  doi = {10.1007/s00221-008-1647-7},
  abstract = {Sudden stretch of active muscle typically results in two characteristic electromyographic responses: the short latency M1 and the long latency M2. The M1 response originates from the monosynaptic Ia afferent reflex pathway. The M2 response is less well understood and is likely a compound response to different afferent inputs mediated by spinal and transcortical pathways. In this study the possible contribution of the Ia afferent pathway to the M2 response was investigated. A mechanism was hypothesized in which the M1 response synchronizes the motoneurons, and therewith their refractory periods. Stretch perturbation experiments were performed on the wrist and results were compared with a computational model of a pool of motoneurons receiving tonic and Ia afferent input. The simulations showed the same stretch amplitude, velocity, and duration-dependent characteristics on the M2 as found experimentally. It was concluded that the stretch duration effect of the M2 likely originates from the proposed Ia afferent mediated mechanism.},
  language = {en},
  number = {4},
  journal = {Experimental Brain Research},
  author = {Schuurmans, Jasper and {de Vlugt}, Erwin and Schouten, Alfred C. and Meskers, Carel G. M. and {de Groot}, Jurriaan H. and {van der Helm}, Frans C. T.},
  month = mar,
  year = {2009},
  keywords = {Ia afferent,M2 response,Stretch reflex},
  pages = {491-500},
  file = {C:\\Users\\rickw\\OneDrive\\Studie\\BMD_Master\\Thesis\\Zotero\\storage\\WNFGPCJX\\Schuurmans et al. - 2009 - The monosynaptic Ia afferent pathway can largely e.pdf},
  note = {00048}
}

@inproceedings{tarata_towards_2017,
  title = {Towards a {{Trial}}-Based,{{Time}}-Scale {{Dynamic Detection}} of {{M1}} and {{M2 Components}} from the {{EMG Stretch Reflex Response}}},
  isbn = {978-3-319-52875-5},
  abstract = {The paper shows a new approach to the dynamic detection of the M1 \& M2 electromyographic stretch reflex evoked activity (SR EMG), based on the detection of abrupt changes within the SR EMG signal on a trial-by-trial basis in the time-domain, using the generalized likelihood ratio test (AGLR). This algorithm was experimented on 16 healthy subjects (8 males and 8 females). The method makes possible the detection of the M1 \& M2 responses in single, individual trials, on a trial-by-trial basis. The novelty which the method brings consists in using only the time-domain to provide the real time detection of the M1 \& M2 in specific experimental closed loop paradigms, comparing to the complexity and time-consumption while using frequency/scale domains.},
