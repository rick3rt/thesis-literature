@incollection{allen_why_2016,
  address = {New York, NY},
  series = {Springer {{Series}} in {{Computational Neuroscience}}},
  title = {Why {{Is Neuromechanical Modeling}} of {{Balance}} and {{Locomotion So Hard}}?},
  isbn = {978-1-4939-3267-2},
  abstract = {A goal and challenge in neuromechanical modeling is to develop validated simulations to predict the effects of neuromotor deficits and therapies on movements. This has been particularly challenging in balance and locomotion because they are inherently unstable, making it difficult to explore model parameters in a way that still coordinates the body in a functional way. Integrating realistic and validated musculoskeletal models with neural control mechanisms is critical to our ability to predict how human robustly move in the environment. Here we briefly review both human locomotion models, which generally focus on modeling the physical dynamics of movement with simplified models of neural control, as well as balance models, which model sensorimotor dynamics and processing with simplified biomechanical models. Combining complex neural and musculoskeletal models increases the redundancy in a model and allows us to study how motor variability and robustness are exploited to produce movements in both healthy and impaired individuals. To advance, the integration of neuromechanical modeling and experimental approaches will be critical in testing specific hypotheses concerning how and why neuromechanical flexibility is both exploited and constrained under various movement contexts. We give a few examples of how the close interplay between models and experiments can reveal neuromechanical principles of movement.},
  language = {en},
  booktitle = {Neuromechanical {{Modeling}} of {{Posture}} and {{Locomotion}}},
  publisher = {{Springer New York}},
  author = {Allen, Jessica L. and Ting, Lena H.},
  editor = {Prilutsky, Boris I. and Edwards, Donald H.},
  year = {2016},
  keywords = {Biomechanics,Balance control,Locomotion,Muscle synergies,Musculoskeletal modeling,Neuromechanics,Postural control,Sensorimotor control,Sensorimotor feedback,Sensorimotor integration},
  pages = {197-223},
  file = {C:\\Users\\rickw\\OneDrive\\Studie\\BMD_Master\\Thesis\\Zotero\\storage\\MFRBT5T3\\Allen_Ting_2016_Why Is Neuromechanical Modeling of Balance and Locomotion So Hard.pdf},
  doi = {10.1007/978-1-4939-3267-2_7},
  note = {00006}
}

@article{klomp_perturbation_2018,
  title = {Perturbation Velocity Affects Linearly Estimated Neuromechanical Wrist Joint Properties},
  volume = {74},
  issn = {0021-9290},
  doi = {10.1016/j.jbiomech.2018.04.007},
  abstract = {The dynamic behavior of the wrist joint is governed by nonlinear properties, yet applied mathematical models, used to describe the measured input-output (perturbation-response) relationship, are commonly linear. Consequently, the linearly estimated model parameters will depend on properties of the applied perturbation properties (such perturbation amplitude and velocity). We aimed to systematically address the effects of perturbation velocity on linearly estimated neuromechanical parameters. Using a single axis manipulator ramp and hold perturbations were applied to the wrist joint. Effects of perturbation velocity (0.5, 1 and 3 rad/s) were investigated at multiple background torque levels (0, 0.5 and 1 N$\cdot$m). With increasing perturbation velocity, estimated joint stiffness remained constant, while damping and reflex gain decreased. This variation in model parameters is dependent on background torque levels, i.e. muscle contraction. These observations support the future development of nonlinear models that are capable of describing wrist joint behavior over a larger range of loading conditions, exceeding the restricted range of operation that is required for linearization.},
  journal = {Journal of Biomechanics},
  author = {Klomp, Asbj\o{}rn and {de Vlugt}, Erwin and {de Groot}, Jurriaan H. and Meskers, Carel G. M. and Arendzen, J. Hans and {van der Helm}, Frans C. T.},
  month = jun,
  year = {2018},
  keywords = {Reflexes,Wrist,Neuromechanics,Nonlinearity,Velocity},
  pages = {207-212},
  file = {C:\\Users\\rickw\\OneDrive\\Studie\\BMD_Master\\Thesis\\Zotero\\storage\\K48BWPGQ\\Klomp et al_2018_Perturbation velocity affects linearly estimated neuromechanical wrist joint.pdf},
  note = {00000}
}

