@article{cronin_ultrasonography_2011,
  title = {Ultrasonography as a Tool to Study Afferent Feedback from the Muscle\textendash{}Tendon Complex during Human Walking},
  volume = {21},
  issn = {1050-6411},
  doi = {10.1016/j.jelekin.2010.08.004},
  abstract = {In humans, one of the most common tasks in everyday life is walking, and sensory afferent feedback from peripheral receptors, particularly the muscle spindles and Golgi tendon organs (GTO), makes an important contribution to the motor control of this task. One factor that can complicate the ability of these receptors to act as length, velocity and force transducers is the complex pattern of interaction between muscle and tendinous tissues, as tendon length is often considerably greater than muscle fibre length in the human lower limb. In essence, changes in muscle\textendash{}tendon mechanics can influence the firing behaviour of afferent receptors, which may in turn affect the motor control. In this review we first summarise research that has incorporated the use of ultrasound-based techniques to study muscle\textendash{}tendon interaction, predominantly during walking. We then review recent research that has combined this method with an examination of muscle activation to give a broader insight to neuromuscular interaction during walking. Despite the advances in understanding that these techniques have brought, there is clearly still a need for more direct methods to study both neural and mechanical parameters during human walking in order to unravel the vast complexity of this seemingly simple task.},
  number = {2},
  journal = {Journal of Electromyography and Kinesiology},
  author = {Cronin, Neil J. and af Klint, Richard and Grey, Michael J. and Sinkjaer, Thomas},
  month = apr,
  year = {2011},
  keywords = {Afferent feedback,Ultrasound,Golgi tendon organ,Human locomotion,Muscle spindle,Muscle–tendon interaction},
  pages = {197-207},
  file = {C:\\Users\\rickw\\OneDrive\\Studie\\BMD_Master\\Thesis\\Zotero\\storage\\8ESBM6GW\\Cronin et al_2011_Ultrasonography as a tool to study afferent feedback from the muscle–tendon.pdf;C:\\Users\\rickw\\OneDrive\\Studie\\BMD_Master\\Thesis\\Zotero\\storage\\9PY2Y7YR\\S1050641110001306.html},
  note = {00015}
}

